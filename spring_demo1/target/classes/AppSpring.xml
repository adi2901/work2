<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"     
    xmlns:context="http://www.springframework.org/schema/context"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd   
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-2.5.xsd">  
   <context:annotation-config/>
   <bean id ="employee" class ="com.spring_demo.Employee">
   <property name="id" value="103"/>
   <property name="name" value="Gourav"/>
   <property name="salary" value="10000"/>
   </bean>
   <bean id ="department" class ="com.spring_demo.Department">
   <property name="id" value="1"/>
   <property name="name" value="EEE"/>
   </bean>
   <bean id ="department1" class ="com.spring_demo.Department">
   <property name="id" value="2"/>
   <property name="name" value="CSE"/>
   </bean>
   <bean id ="app" class ="com.spring_demo.App"/>
   
