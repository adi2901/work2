   <bean id ="app" class ="org.gourav.TestSpring.config.App" autowire="byType"/> 
    <bean id ="app" class ="org.gourav.TestSpring.config.App" autowire="byName"/> 
    <bean id ="app" class ="org.gourav.TestSpring.config.App" autowire="constructor"/>
    -->
    
   </beans>